{
  "name": "vanilla-js-spa",
  "version": "1.0.0",
  "description": "Private repository for @alexey-koren",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --open --hot",
    "build": "webpack --mode=production",
    "prettier:format": "prettier --write \"**/*.+(js|(sa|sc)ss)\"",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:fix-staged": "eslint --fix",
    "fix": "run-s prettier:format lint:fix",
    "fix-staged": "lint-staged",
    "svgo": "cross-env svgo --pretty --indent=2 --disable=removeViewBox,mergePaths --enable=inlineStyles --config svgo-config.json $FILE",
    "svgo-all": "cross-env svgo -r -f src/assets/svg --pretty --indent=2 --disable=removeViewBox,mergePaths --enable=inlineStyles --config svgo-config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alexey-Koren/webpack-boilerplate.git"
  },
  "author": "Alexey-Koren",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alexey-Koren/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/Alexey-Koren/webpack-boilerplate/blob/simple-scss-js/README.md",
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "browser-sync": "^2.27.7",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.3",
    "file-loader": "^6.2.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.2",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "svgo": "^1.3.2",
    "webpack": "^5.64.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  }
}
